server {
  listen #{PROJECT_NAME}:80;
  server_name #{PROJECT_NAME};

  access_log  /var/log/nginx/#{PROJECT_NAME}.access.log;
  set_real_ip_from 0.0.0.0/0;
  real_ip_header X-Real-IP;

  location / {
    alias /usr/local/lib/luarocks/rocks/#{PROJECT_NAME}.www.static/scm-1/www/#{PROJECT_NAME}/static/;
  }
}
