server {
  listen pk-logiceditor-com-internal-config-deploy:80;
  server_name pk-logiceditor-com-internal-config-deploy;

  access_log  /var/log/nginx/pk-logiceditor-com-internal-config-deploy.access.log;
  set_real_ip_from 0.0.0.0/0;
  real_ip_header X-Real-IP;

  location / {
    proxy_pass http://pk-logiceditor-com-internal-config/;
  }

  location /cfg/db/bases {
    alias /usr/local/lib/luarocks/rocks/pk-logiceditor-com.internal-config-deploy.localhost-ag/scm-1/cluster/localhost-ag/internal-config/internal-config-deploy/cfg/db/bases;
  }
}
