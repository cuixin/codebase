server {
  listen pk-logiceditor-com:80;
  server_name pk-logiceditor-com;

  access_log  /var/log/nginx/pk-logiceditor-com.access.log;
  set_real_ip_from 0.0.0.0/0;
  real_ip_header X-Real-IP;

  location / {
    alias /usr/local/lib/luarocks/rocks/pk-logiceditor-com.www.static/scm-1/www/logiceditor.com/static/;
  }
}
